{
  "description": "Bytecoding 简化版开发工作流 hooks",
  "hooks": {
    "SessionStart": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "node \"${CLAUDE_PLUGIN_ROOT}/scripts/session-start-hook.js\"",
            "timeout": 10
          }
        ]
      }
    ]
  }
}
